{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-sonarts",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [true, ".prettierrc.yaml"],
    "cognitive-complexity": false,
    "no-big-function": false,
    "prefer-const": true,
    "prefer-template": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
